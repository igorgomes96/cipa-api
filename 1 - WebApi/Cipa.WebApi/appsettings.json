{
  "AWS": {
    "Region": "us-east-2"
  },
  "Logging": {
    "Region": "us-east-2",
    "LogGroup": "Orion.API",
    "IncludeLogLevel": true,
    "IncludeCategory": true,
    "IncludeNewline": true,
    "IncludeException": true,
    "IncludeEventId": false,
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TokenConfigurations": {
    "Audience": "SistemaCIPA",
    "Issuer": "SistemaCIPA",
    "Seconds": 6000
  },
  "Email": {
    "Host": "smtp.gmail.com",
    "Port": 587,
    "EnabledSSL": true,
    "UseDefaultCredentials": false,
    "Alias": "cipa@4uptech.com.br"
  },
  "Importacao": {
    "Validators": [
      {
        "ColumnName": "Matrícula",
        "MaxLength": 20,
        "Required": false,
        "DataType": "String"
      },
      {
        "ColumnName": "Email",
        "MaxLength": 100,
        "Required": true,
        "DataType": "Email"
      },
      {
        "ColumnName": "Nome",
        "MaxLength": 255,
        "Required": true,
        "DataType": "String"
      },
      {
        "ColumnName": "Cargo",
        "MaxLength": 255,
        "Required": false,
        "DataType": "String"
      },
      {
        "ColumnName": "Área",
        "MaxLength": 255,
        "Required": false,
        "DataType": "String"
      },
      {
        "ColumnName": "Data de Admissão",
        "Required": false,
        "DataType": "Date"
      },
      {
        "ColumnName": "Data de Nascimento",
        "Required": false,
        "DataType": "Date"
      },
      {
        "ColumnName": "Nome do Gestor",
        "MaxLength": 255,
        "Required": false,
        "DataType": "String"
      },
      {
        "ColumnName": "Email do Gestor",
        "MaxLength": 100,
        "Required": false,
        "DataType": "Email"
      }
    ]
  }
}
